{
  "version": 2,
  "env": {
    "NODE_VERSION": "20"
  },
  "buildCommand": "cd cyberx-frontend && npm install && npm run build",
  "outputDirectory": "cyberx-frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "cyberx-frontend/dist/index.html"
    }
  ]
}
